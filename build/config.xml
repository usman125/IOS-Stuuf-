<?xml version='1.0' encoding='utf-8'?>
<widget id="com.stuuf.test" version="0.0.22" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Stuuf</name>
    <description />
    <preference name="phonegap-version" value="cli-8.1.1" />
    <engine name="android" spec="7.1.3" />
    
    <access origin="*" />
    <allow-navigation href="*://*.mendix.com/*" />
    <allow-navigation href="*://*.mendixcloud.com/*" />
    <allow-navigation href="*://*.mxapp.io/*" />
    <allow-navigation href="*://*.stuuf.io/*" />
    <allow-navigation href="about:*" />
    <allow-navigation href="*://js.stripe.com/*" />
    <allow-navigation href="*://m.stripe.network/*" />
    <allow-navigation href="*://connect.stripe.com/*" />
    <allow-navigation href="*://checkout.stripe.com/*" />

    <plugin name="com.darktalker.cordova.screenshot" source="npm" spec="0.1.6" />
    <plugin name="com.telerik.plugins.nativepagetransitions" source="npm" spec="0.6.5" />
    <plugin name="cordova-build-architecture" source="git" spec="https://github.com/MBuchalik/cordova-build-architecture.git#v1.0.4" />
    <plugin name="cordova-plugin-actionsheet" source="npm" spec="2.3.3" />
    <plugin name="cordova-plugin-app-version" source="npm" spec="0.1.9" />
    <plugin name="cordova-plugin-battery-status" source="npm" spec="2.0.2" />
    <plugin name="cordova-plugin-cookieemperor" spec="https://github.com/rtk/cordova-cookie-emperor.git" />
    <plugin name="cordova-plugin-device" source="npm" spec="2.0.2" />
    <plugin name="cordova-plugin-device-motion" source="npm" spec="2.0.1" />
    <plugin name="cordova-plugin-device-orientation" source="npm" spec="2.0.1" />
    <plugin name="cordova-plugin-dialogs" source="npm" spec="2.0.1" />
    <plugin name="cordova-plugin-file" source="npm" spec="6.0.1" />
    <plugin name="cordova-plugin-file-opener2" source="npm" spec="2.0.19" />
    <plugin name="cordova-plugin-file-transfer" source="npm" spec="1.7.1" />
    <plugin name="cordova-plugin-globalization" source="npm" spec="1.11.0" />
    <plugin name="cordova-plugin-inappbrowser" source="npm" spec="3.0.0" />
    <plugin name="cordova-plugin-network-information" source="npm" spec="2.0.1" />
    <plugin name="cordova-plugin-secure-storage" source="npm" spec="2.6.8" />
    <plugin name="cordova-plugin-spinner" source="npm" spec="1.1.0" />
    <plugin name="cordova-plugin-splashscreen" source="npm" spec="5.0.2" />
    <plugin name="cordova-plugin-statusbar" source="npm" spec="2.4.2" />
    <plugin name="cordova-plugin-vibration" source="npm" spec="3.1.0" />
    <plugin name="cordova-plugin-x-socialsharing" source="npm" version="5.4.0" />
    <plugin name="cordova-plugin-zip" source="npm" spec="3.1.0" />
    <plugin name="@mendix/cordova-sqlite-storage" source="npm" spec="2.0.4-mx.1.1.0" />
    <plugin name="@mendix/uk.co.workingedge.phonegap.plugin.launchnavigator" source="npm" spec="4.2.2-mx.1.0.0" />
    <plugin name="cordova-universal-links-plugin" source="git" spec="https://github.com/walteram/cordova-universal-links-plugin" />
    <universal-links>
        <host name="app.stuuf.io" scheme="https">
            <path event="launchmyapp" url="*launchmyapp*" />
            <path event="myorders" url="*myorders*" />
            <path event="myproducts" url="*myproducts*" />
            <path event="mysales" url="*mysales*" />
            <path event="offers" url="*offers*" />
            <path event="orderchat" url="*orderchat*" />
        </host>
    </universal-links>
    <plugin name="cordova-plugin-calendar" source="npm" spec="5.1.2">
        <param name="CALENDAR_USAGE_DESCRIPTION" value="This app needs to access your calendar" />
    </plugin>
    <plugin name="cordova-plugin-camera" source="npm" spec="4.0.3" />
    <plugin name="cordova-plugin-contacts" source="npm" spec="3.0.1" />
    <plugin name="cordova-plugin-media-capture" source="npm" spec="3.0.2" />
    <plugin name="phonegap-plugin-barcodescanner" source="npm" spec="8.0.0" />
    <plugin name="cordova-plugin-geolocation" source="npm" spec="4.0.1" />
    <plugin name="cordova-plugin-media" source="npm" spec="5.0.2" />
    <plugin name="phonegap-plugin-push" source="npm" spec="2.2.3" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#000000" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="webviewbounce" value="false" />
    <preference name="iosPersistentFileLocation" value="Library" />
    <preference name="KeyboardDisplayRequiresUserAction" value="false" />
    <preference name="BackupWebStorage" value="none" />
    <feature name="CDVWKWebViewEngine">
        <param name="ios-package" value="CDVWKWebViewEngine" />
    </feature>
    <preference name="CordovaWebViewEngine" value="CDVWKWebViewEngine" />
    <preference name="KeyboardDisplayRequiresUserAction" value="false" />
    <platform name="ios">
        <icon height="114" src="res/ios/icon-2x.png" width="114" />
        <icon height="87" src="res/ios/icon-small-3x.png" width="87" />
        <icon height="76" src="res/ios/icon-76.png" width="76" />
        <icon height="57" src="res/ios/icon.png" width="57" />
        <icon height="100" src="res/ios/icon-50-2x.png" width="100" />
        <icon height="152" src="res/ios/icon-76-2x.png" width="152" />
        <icon height="60" src="res/ios/icon-60.png" width="60" />
        <icon height="120" src="res/ios/icon-60-2x.png" width="120" />
        <icon height="29" src="res/ios/icon-small.png" width="29" />
        <icon height="1024" src="res/ios/iTunesArtwork-2x.png" width="1024" />
        <splash height="320" src="res/ios/screen-iphone-landscape.png" width="480" />
        <splash height="2208" src="res/ios/screen-iphone-portrait-736h.png" width="1242" />
        <splash height="1024" src="res/ios/screen-ipad-portrait.png" width="768" />
        <splash height="640" src="res/ios/screen-iphone-landscape-2x.png" width="960" />
        <icon height="167" src="res/ios/icon-83.5-2x.png" width="167" />
        <splash height="768" src="res/ios/screen-ipad-landscape.png" width="1024" />
        <splash height="1334" src="res/ios/screen-iphone-portrait-667h.png" width="750" />
        <icon height="80" src="res/ios/icon-40-2x.png" width="80" />
        <icon height="50" src="res/ios/icon-50.png" width="50" />
        <splash height="1536" src="res/ios/screen-ipad-landscape-2x.png" width="2048" />
        <splash height="1136" src="res/ios/screen-iphone-portrait-568h-2x.png" width="640" />
        <splash height="750" src="res/ios/screen-iphone-landscape-667h.png" width="1334" />
        <icon height="180" src="res/ios/icon-60-3x.png" width="180" />
        <splash height="2048" src="res/ios/screen-ipad-portrait-2x.png" width="1536" />
        <icon height="40" src="res/ios/icon-40.png" width="40" />
        <splash height="640" src="res/ios/screen-iphone-landscape-568h-2x.png" width="1136" />
        <icon height="58" src="res/ios/icon-small-2x.png" width="58" />
        <splash height="960" src="res/ios/screen-iphone-portrait-2x.png" width="640" />
        <icon height="144" src="res/ios/icon-72-2x.png" width="144" />
        <icon height="72" src="res/ios/icon-72.png" width="72" />
        <splash height="1242" src="res/ios/screen-iphone-landscape-736h.png" width="2208" />
        <splash height="480" src="res/ios/screen-iphone-portrait.png" width="320" />
        <splash height="1242" src="res/ios/screen-iphone-landscape-2688h-3x.png" width="2688" />
        <splash height="2688" src="res/ios/screen-iphone-portrait-2688h-3x.png" width="1242" />
        <edit-config file="*-Info.plist" mode="merge" target="NSCameraUsageDescription">
            <string>This app needs to access your camera to upload your product images and scan QR code to complete your order  delivery</string>
        </edit-config>
        <edit-config file="*-Info.plist" mode="merge" target="NSPhotoLibraryUsageDescription">
            <string>This app needs to access your photo library to upload your product images</string>
        </edit-config>
        <resource-file src="GoogleService-Info.plist" />
        <edit-config file="*-Info.plist" mode="merge" target="NSContactsUsageDescription">
            <string>This app needs to access your contacts</string>
        </edit-config>
        <edit-config file="*-Info.plist" mode="merge" target="NSMicrophoneUsageDescription">
            <string>This app needs to access your microphone</string>
        </edit-config>
        <edit-config file="*-Info.plist" mode="merge" target="NSLocationWhenInUseUsageDescription">
            <string>This app needs to access your location</string>
        </edit-config>
        <edit-config target="NSLocationAlwaysUsageDescription" file="*-Info.plist" mode="merge">
            <string>This app needs to access your location</string>
        </edit-config>
        <edit-config target="NSCalendarsUsageDescription" file="*-Info.plist" mode="merge">
            <string>This app needs to access your calendar</string>
        </edit-config>
    </platform>
    <splash src="res/android/screen-xxxhdpi-portrait.png" />
    <engine name="ios" spec="~4.5.5" />
</widget>
